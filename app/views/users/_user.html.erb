<div class="row">
  <div class="col-md-6 col-md-offset-3 users">
    <%= link_to user.name, user %>
    <% if current_user.is_admin? && !current_user?(user) %>
        | <%= link_to "delete", user, method: :delete,
                      data: { confirm: "You sure?" } %>
    <% end %>
  </div>
</div>
