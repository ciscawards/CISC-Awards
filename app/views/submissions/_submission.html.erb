<tr>
  <td><%= submission.name %></td>
  <td><%= submission.project_location %></td>
  <td><%= submission.submission_categories.map{|submission_category| submission_category.category.title}.join(", ") %></td>
  <td>
    <%= link_to 'view', submission_path(submission) %>
    | <%= link_to 'edit', edit_submission_path(submission) %>
    | <%= link_to "delete", submission, method: :delete, data: { confirm: "You sure?" } %>
  </td>
</tr>